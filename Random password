import random
import string

def get_random_string(length):
    letters = string.ascii_lowercase
    result_str = ''.join(random.choice(letters) for i in range(length))
    print("Parola aleatorie de", length, "cifre este:", result_str)
cifre=input('Din cate cifre sa fie formata parola?: ')
get_random_string(int(cifre))

input("Press ENTER to exit")
